<Window x:Class="Week5Lab.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Week5Lab"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Canvas HorizontalAlignment="Left" Height="399" Margin="10,10,0,0" VerticalAlignment="Top" Width="772">
            <TextBox x:Name="YourSpeedTextBox" Height="23" Canvas.Left="505" TextWrapping="Wrap" Text="" Canvas.Top="167" Width="120"/>
            <TextBox x:Name="SpeedLimitTextBox" Height="23" Canvas.Left="111" TextWrapping="Wrap" Text="" Canvas.Top="167" Width="139" RenderTransformOrigin="0.508,0.457"/>
            <Label x:Name="YourSpeedLabel" Content="Your Speed" Canvas.Left="418" Canvas.Top="167" Height="23" Width="66"/>
            <Label x:Name="SpeedLimitLabel" Content="Speed Limit" Canvas.Left="10" Canvas.Top="167" Width="101" Height="23" RenderTransformOrigin="0.969,0.587"/>
            <Button x:Name="Button" Content="Compare Speeds" Canvas.Left="316" Canvas.Top="254" Width="140" Height="36" Click="Button_Click"/>
            <Label x:Name="ResultLabel" Content="" Canvas.Left="226" Canvas.Top="55" Height="59" FontSize="40" Width="358"/>
        </Canvas>

    </Grid>
</Window>
